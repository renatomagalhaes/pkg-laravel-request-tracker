{
    "name": "renatomagalhaes/pkg-laravel-request-tracker",
    "description": "A Laravel package for tracking and monitoring HTTP requests.",
    "license": "MIT",
    "keywords": [
        "renatomagalhaes",
        "laravel",
        "request",
        "tracker",
        "logs",
        "monitoring",
        "performance",
        "security"
    ],
    "autoload": {
        "psr-4": {
            "RenatoMagalhaes\\RequestTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenatoMagalhaes\\RequestTracker\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Renato Magalh√£es",
            "email": "rsmdbmaster@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4|^10.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "RenatoMagalhaes\\RequestTracker\\Providers\\RequestTrackerServiceProvider"
            ]
        }
    }
}
